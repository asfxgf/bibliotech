<p id="notice"><%= notice %></p>

<h1>Appartenances</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @appartenances.each do |appartenance| %>
      <tr>
        <td><%= link_to 'Show', appartenance %></td>
        <td><%= link_to 'Edit', edit_appartenance_path(appartenance) %></td>
        <td><%= link_to 'Destroy', appartenance, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Appartenance', new_appartenance_path %>
